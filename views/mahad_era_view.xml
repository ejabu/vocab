<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record model="ir.actions.act_window" id="mahad_era_action">
            <field name="name">Era</field>
            <field name="res_model">mahad.era</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">Era
                </p>
            </field>
        </record>

        <menuitem id="sub_mahad_era" sequence="20" name="Era" parent="vole_master_data" action="mahad_era_action"/>

        <!-- Tree view for the menuitem -->
        <record id="mahad_era_tree" model="ir.ui.view">
            <field name="name">mahad.era.tree</field>
            <field name="model">mahad.era</field>
            <field name="arch" type="xml">
                <tree editable="top" string="Tree">
                    <field name="name"/>
                    <field name="era"/>
                    <field name="year_enter"/>
                </tree>
            </field>
        </record>


    </data>
</openerp>
